{
  "compilerOptions": {
    "allowJs": true,
    "target": "ESnext",
    "module": "ESnext",
    "lib": ["ESnext", "DOM", "WebWorker", "DOM.Iterable"],
    "jsx": "react-jsx",
    "jsxImportSource": "@builder.io/qwik",
    "strict": false,
    
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
  
    
    "outDir": "tmp",
    
    "types": ["node", "vite/client"],
    "paths": {
      "~/*": ["./src/*"]
    }
  }
}
